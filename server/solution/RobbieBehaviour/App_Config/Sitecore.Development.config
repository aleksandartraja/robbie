<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<sitecore xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" >  
  <linkManager>
    <providers>
      <clear/>
      <add name="sitecore" languageEmbedding="never" lowercaseUrls="true" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </providers>
  </linkManager>
  <setting name="HtmlEditor.DefaultProfile" value="/sitecore/system/Settings/Html Editor Profiles/Rich Text Medium" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
</sitecore>
